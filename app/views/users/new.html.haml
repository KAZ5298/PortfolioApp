%h1
  Registrations#new
%p Find me in app/views/registrations/new.html.erb
= form_for @user, url: sign_up_path, method: :post do |f|
  = f.label :name
  = f.text_field :name
  = f.label :email
  = f.text_field :email
  = f.label :password
  = f.password_field :password
  = f.label :password_confirmation
  = f.password_field :password_confirmation
  = f.submit
